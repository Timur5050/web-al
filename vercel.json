{
    "version": 2,
    "builds": [
      {
        "src": "back_al/src/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "back_al/public/build/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/vacancies(.*)",
        "dest": "back_al/src/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "back_al/public/build/index.html"
      }
    ],
    "installCommand": "npm install",
    "buildCommand": "cd web_al/job-platform && npm install && npm run build && mkdir -p $PWD/../../back_al/public/build && cp -r build/* $PWD/../../back_al/public/build && ls -la $PWD/../../back_al/public/build && pwd",
    "outputDirectory": "back_al/public/build"
  }